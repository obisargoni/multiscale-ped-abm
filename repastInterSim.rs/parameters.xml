<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="10"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
					<parameter name="pedODSeed" displayName="Pedestrian OD Seed" type="int" 
						defaultValue="100"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
					<parameter name="vehODSeed" displayName="Vehicle OD Seed" type="int" 
						defaultValue="101"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="avNVehicles" displayName="Level of vehicle agents" type="int" 
						defaultValue="50" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="addPedTicks" displayName="Ticks between pedestrian creation" type="double" 
						defaultValue="2.5" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>

						<parameter name="alpha" displayName="Controls weighting betweem traffic exposure and journey time" type="double" 
						defaultValue="0.5" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
								<parameter name="lambda" displayName="Controls the effect of salience distance on sampling probability" type="double" 
						defaultValue="1.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
								<parameter name="gamma" displayName="Activation decay factor" type="double" 
						defaultValue="0.9" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
								<parameter name="epsilon" displayName="Activation threshold for crossing choice" type="double" 
						defaultValue="6.5" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
								<parameter name="ga" displayName="Gap acceptance parameter" type="double" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
								<parameter name="minCrossing" displayName="Whether pedestrian agents minimising no. crossings" type="boolean" 
						defaultValue="false" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>
								<parameter name="tacticalPlanHorizon" displayName="Cumulative angular distance pedestrian percieves at tactical level" type="double" 
						defaultValue="200" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
								<parameter name="nPeds" displayName="Total number of pedestrians to add to the model" type="int" 
						defaultValue="200" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
								<parameter name="timeThreshold" displayName="Time threshold to trigger crossing alternative choice at (direct crossing only)" type="int" 
						defaultValue="60" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
								<parameter name="yieldThreshold" displayName="Yielding time at which to trigger choosing new crossing location" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
								<parameter name="updateFactor" displayName="Factor by which to update alpha dn gap acceptance parameters when ped has failed to cross" type="double" 
						defaultValue="1.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
								<parameter name="informalCrossing" displayName="Control whether to always, sometimes or never allow informal crossing" type="String" 
						defaultValue="always" 
						isReadOnly="false" 
						
									
		/>
		</parameters>


